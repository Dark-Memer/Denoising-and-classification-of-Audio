      "source": [
        "def multiplicate(a):\n",
        "  b = []\n",
        "  for i in range(len(a)):\n",
        "    mul = 1\n",
        "    for j in range(len(a)):\n",
        "      if j != i:\n",
        "        mul = a[j] * mul\n",
        "    b.append(mul)\n",
        "  return b"
      ]
    },
